<query id="insertNmsSeverityLog" action="insert">
    <tables>
        <table name="nms_severity_log" />
    </tables>
    <columns>
        <column name="module_srl" var="module_srl" notnull="notnull" />
        <column name="mib_srl" var="mib_srl" notnull="notnull" />
        <column name="severity" var="severity" notnull="notnull" />
        <column name="value" var="value" />
        <column name="aware" var="aware" default="0" notnull="notnull" />
        <column name="awaredate" var="awaredate" />
        <column name="regdate" var="regdate" default="curdate()" />
        <column name="extra_vars" var="extra_vars" />
    </columns>
</query>