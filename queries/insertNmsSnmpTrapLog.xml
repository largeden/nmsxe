<query id="insertNmsSnmpTrapLog" action="insert">
    <tables>
        <table name="nms_snmptrap_log" />
    </tables>
    <columns>
        <column name="trap" var="trap" filter="number" notnull="notnull" />
        <column name="ip_type" var="ip_type" notnull="notnull" />
        <column name="ip_address" var="ip_address" notnull="notnull" />
        <column name="ip_port" var="ip_port" notnull="notnull" />
        <column name="value" var="value" default="" />
        <column name="regdate" var="regdate" default="curdate()" />
        <column name="extra_vars" var="extra_vars" default="" />
    </columns>
</query>