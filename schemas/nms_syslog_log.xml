    <column name="priority" type="char" size="3" notnull="notnull" />
    <column name="facility" type="char" size="2" notnull="notnull" />
    <column name="severity" type="char" size="1" notnull="notnull" />
    <column name="ip_type" type="char" size="1" notnull="notnull" />
    <column name="ip_address" type="varchar" size="250" notnull="notnull" />
    <column name="ip_port" type="char" size="5" notnull="notnull" />
    <column name="value" type="varchar" size="250" notnull="notnull" />
    <column name="regdate" type="date" notnull="notnull" index="idx_regdate" />
    <column name="extra_vars" type="text" />
</table>